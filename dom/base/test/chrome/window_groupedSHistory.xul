    // bg1: pineapple
    // bg2: apple-pencil
    .then(() => info('TEST-INFO | test content history go back #3, remote=' + remote))
    .then(() => wrapHistoryNavFn(act, contentGoBack.bind(null, act), 'pineapple', 'pen', bg1))
    .then(() => verifyBrowser(act, 'pen'       /* title */,
