    .then(() => info('TEST-INFO | test content history go #3, remote=' + remote))
    .then(() => wrapHistoryNavFn(act, contentGo.bind(null, act, -1), 'apple', 'pineapple', bg2))
    .then(() => verifyBrowser(act, 'pineapple' /* title */,
                                    0          /* index */,
