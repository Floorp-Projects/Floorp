include protocol PGMP;

namespace mozilla {
namespace gmp {

async protocol PGMPTimer
{
  manager PGMP;
child:
  async TimerExpired(uint32_t aTimerId);
parent:
  async SetTimer(uint32_t aTimerId, uint32_t aTimeoutMs);
  async __delete__();
};

} // namespace gmp
} // namespace mozilla
