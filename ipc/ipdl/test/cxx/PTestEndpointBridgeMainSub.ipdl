include protocol PTestEndpointBridgeMain;
include protocol PTestEndpointBridgeSub;

namespace mozilla {
namespace _ipdltest {

// (Bridge protocols can have different semantics than the endpoints
// they bridge)
intr protocol PTestEndpointBridgeMainSub {
child:
  Hi();
  intr HiRpc();

parent:
  Hello();
  sync HelloSync();
  intr HelloRpc();
};


} // namespace mozilla
