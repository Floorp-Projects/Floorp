include protocol PTestDataStructuresSub;

using mozilla::null_t;
using nsIntRegion;

namespace mozilla {
namespace _foo {

union IntDouble {
    int;
    double;
};

struct SIntDouble {
    int i;
    double d;
};

union IntDoubleArrays {
    int;
    int[];
    double[];
};

struct SIntDoubleArrays {
