include protocol PTestBridgeMainSub;
include protocol PTestBridgeSub;

namespace mozilla {
namespace _ipdltest {


protocol PTestBridgeMain {
    child spawns PTestBridgeSub;
    child opens PTestBridgeMainSub;

child:
    async Start();

parent:
    async __delete__();

state START:
    send Start goto DEAD;
state DEAD:
    recv __delete__;
};


} // namespace mozilla
} // namespace _ipdltest
