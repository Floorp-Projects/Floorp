include protocol PTestOpensOpened;

namespace mozilla {
namespace _ipdltest {


protocol PTestOpens {
    // This channel is opened and parked on a non-main thread
    child opens PTestOpensOpened;

child:
    Start();

parent:
    __delete__();

state START:
    send Start goto DEAD;
state DEAD:
    recv __delete__;
};


} // namespace mozilla
} // namespace _ipdltest
