# Bug 1294650 - populate our install with a shim dll to work around a
# 3rd party code injection crash.

SOURCES += [
    "dummydll.cpp",
]

if CONFIG["CPU_ARCH"] == "x86_64":
    GeckoSharedLibrary("qipcap64")
else:
    GeckoSharedLibrary("qipcap")

NoVisibilityFlags()
NO_PGO = True
