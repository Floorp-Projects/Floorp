INTERNAL_TOOLS = 1
NO_PROFILE_GUIDED_OPTIMIZE = 1

include $(topsrcdir)/config/rules.mk

export:: $(CSRCS:.c=.$(OBJ_SUFFIX))

$(CSRCS): %.c: ../inject.c
	cp $< $@

GARBAGE += $(CSRCS)

CFLAGS := -O2 -fno-stack-protector $(filter -m% -I%,$(CFLAGS))
