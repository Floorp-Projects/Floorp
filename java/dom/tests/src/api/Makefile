clean:
	for i in ${JAVAFILES} ; do \
		file=$$i; \
		classfile=`echo $$file | sed 's/java/class/g'` ; \
		/bin/rm $(CLASSDIR)/$(TESTPACKAGE)/$$classfile ; done
