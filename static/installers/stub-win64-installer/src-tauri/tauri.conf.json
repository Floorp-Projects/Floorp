{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "floorp-win64-stub-installer",
  "version": "0.1.0",
  "identifier": "app.stubInstaller.floorp",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Floorp Installer",
        "width": 800,
        "height": 600,
        "decorations": false,
        "resizable": false,
        "closable": true,
        "maximizable": true,
        "minimizable": true
      }
    ],
    "security": {
      "csp": null
    },
    "withGlobalTauri": true
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/icon.ico"
    ]
  }
}