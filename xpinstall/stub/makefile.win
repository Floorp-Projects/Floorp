DEPTH=..\..

REQUIRES	= xpcom \
		  string \
		  dom \
		  appshell \
		  appcomps \
		  xpinstall \
		  $(NULL)
MAKE_OBJ_TYPE   = DLL
LIBRARY_NAME=xpistub
DLL = .\$(OBJDIR)\xpistub.dll

OBJS =  \
    .\$(OBJDIR)\nsStubNotifier.obj \
	.\$(OBJDIR)\xpistub.obj \
	$(NULL)

LLIBS =                        \
    $(LIBNSPR)                 \
    $(DIST)\lib\xpcom.lib      \
    $(NULL)

include <$(DEPTH)\config\rules.mak>

install:: $(DLL)
	$(MAKE_INSTALL) $(DLL) $(DIST)\bin
 
clobber::
	$(RM) $(DIST)\bin\xpistub.dll
