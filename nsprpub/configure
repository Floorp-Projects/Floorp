*-*-sco*)
    $as_echo "#define XP_UNIX 1" >>confdefs.h

    $as_echo "#define SCO 1" >>confdefs.h

    $as_echo "#define sco 1" >>confdefs.h

    $as_echo "#define SYSV 1" >>confdefs.h

