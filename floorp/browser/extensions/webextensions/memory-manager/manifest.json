{
  "manifest_version": 2,
  "name": "Memory Manager",
  "description": "",
  "version": "1.0",
  "permissions": [
    "tabs",
    "<all_urls>"
  ],
  "experiment_apis": {
    "aboutConfigPrefs": {
        "schema": "API/aboutConfigPrefs.json",
        "parent": {
            "scopes": ["addon_parent"],
            "script": "API/aboutConfigPrefs.js",
            "paths": [["aboutConfigPrefs"]]
        }
    },
    "memoryInfo": {
      "schema": "API/memoryInfo.json",
