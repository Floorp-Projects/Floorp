{
  "manifest_version": 2,
  "name": "Web Panel User-Agent",
  "version": "1.0",
  "icons": {
    "48": "link-48.png"
  },
  "applications": {
    "gecko": {
    "id": "webpanel-ua@floorp.ablaze.one"
		}
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  
  "background": {
    "scripts": ["background.js"]
  },
  "experiment_apis": {
    "webRequestExt": {
      "schema": "APIs/webRequestExt.json",
      "parent": {
        "scopes": [
          "addon_parent"
        ],
        "script": "APIs/webRequestExt.js",
        "paths": [["webRequestExt"]]
      }
    }
  }
}
