<script language=javascript>
var dosubmit=false;
var doclose=false;

function submitform()
{
  if ({$1}())
    dosubmit = true;
  window.frames[1].submitForm();
}

function closewin()
{
  doclose = true;
  setTimeout("closeif()", 100);	
}

function closeif()
{
  if (doclose) {
    window.close();
  }
  setTimeout("closeif()", 100);
}


</script>
