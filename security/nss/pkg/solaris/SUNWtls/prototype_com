d none usr/lib 755 root bin
d none usr/lib/mps 755 root bin
d none usr/lib/mps/secv1 755 root bin
f none usr/lib/mps/libnss3.so 755 root bin
f none usr/lib/mps/libsmime3.so 755 root bin
