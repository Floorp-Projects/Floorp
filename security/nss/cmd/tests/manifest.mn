CORE_DEPTH = ../..

# MODULE public and private header  directories are implicitly REQUIRED.
MODULE = nss

CSRCS = \
	baddbdir.c \
	conflict.c \
	dertimetest.c \
	encodeinttest.c \
	nonspr10.c \
	remtest.c \
	secmodtest.c \
	$(NULL)

# The MODULE is always implicitly required.
# Listing it here in REQUIRES makes it appear twice in the cc command line.
REQUIRES = seccmd dbm

PROGRAMS = $(CSRCS:.c=)

TARGETS = $(PROGRAMS)

NO_MD_RELEASE = 1
