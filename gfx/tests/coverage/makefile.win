DEPTH=..\..\..

MAKE_OBJ_TYPE   = EXE
PROGRAM = .\$(OBJDIR)\coverage.exe
#RESFILE = coverage.res

OBJS =  \
	.\$(OBJDIR)\main.obj        \
	.\$(OBJDIR)\nsCoverage.obj   \
	$(NULL)

LINCS=-I$(PUBLIC)\raptor \
      -I$(PUBLIC)\xpcom -I\ns\raptor\ui\src\windows

LLIBS=                         \
 $(DIST)\lib\xpcom.lib        \
 $(LIBNSPR)                     \
 $(DIST)\lib\widgetsupport_s.lib \
 $(DIST)\lib\raptorwidget.lib   \
 $(DIST)\lib\raptorgfxwin.lib   \
 $(NULL)

LLFLAGS=-SUBSYSTEM:CONSOLE 

include <$(DEPTH)\config\rules.mak>

install:: $(PROGRAM)
	$(MAKE_INSTALL) $(PROGRAM) $(DIST)\bin

clobber::
	rm -f $(DIST)\bin\coverage.exe

$(PROGRAM):: $(OBJS) $(MYLIBS)
