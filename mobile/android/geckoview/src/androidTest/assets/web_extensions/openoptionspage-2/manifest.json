{
  "manifest_version": 2,
  "name": "openOptionsPage-2",
  "version": "1.0",
  "description": "Opens options page via delegate.",
  "browser_specific_settings": {
    "gecko": {
      "id": "openoptionspage2@tests.mozilla.org"
    }
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": ["tabs"],
  "options_ui": {
    "page": "options.html",
    "browser_style": true,
    "open_in_tab": false
  }
}
