{
  "manifest_version": 2,
  "name": "Test support",
  "version": "1.0",
  "description": "Helper script for GeckoView tests",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["test-support.js"],
      "run_at": "document_start"
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "geckoViewAddons",
    "nativeMessaging"
  ]
}
