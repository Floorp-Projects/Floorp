DEPTH=..\..\..\..
MODULE=xmlextras
REQUIRES	= xpcom \
		  string \
		  dom \
		  js \
		  widget \
		  caps \
		  necko \
		  webBrowser_core \
		  appstartup \
		  xpconnect \
		  content \
		  $(NULL)
LIBRARY_NAME=xmlextras
MODULE_NAME=nsXMLExtrasModule

################################################################################
## library

LINCS=-I..\..\base\src -I..\..\soap\src -I..\..\schema\src -I..\..\wsdl\src

CPP_OBJS=					    \
		.\$(OBJDIR)\nsXMLExtrasModule.obj   \
		$(NULL)

SUB_LIBRARIES=                                        \
                $(DIST)\lib\xmlextrasbase_s.lib       \
!if defined(MOZ_SOAP)
                $(DIST)\lib\xmlextrassoap_s.lib       \
!endif
!if defined(MOZ_WSDL)
		$(DIST)\lib\xmlextrasschema_s.lib      \
		$(DIST)\lib\xmlextraswsdl_s.lib        \
!endif
                $(NULL)

LLIBS=						      \
		$(DIST)\lib\xpcom.lib		      \
		$(DIST)\lib\js3250.lib		      \
		$(LIBNSPR)	 	              \
		$(NULL)

!if defined(MOZ_SOAP)
DEFINES = -DMOZ_SOAP
!endif
!if defined(MOZ_WSDL)
DEFINES =$(DEFINES) -DMOZ_SCHEMA -DMOZ_WSDL
!endif

LCFLAGS = \
        $(LCFLAGS) \
        $(DEFINES) \
        $(NULL)

include <$(DEPTH)/config/rules.mak>

