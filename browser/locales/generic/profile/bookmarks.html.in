#filter substitution
#include @BOOKMARKS_INCLUDE_DIR@/bookmarks.inc
#define ja_jp_mac ja-JP-mac
#if AB_CD == ja_jp_mac
#define AB_CD ja
#endif

#define mozilla_icon data:image/png;base64,

#define nightly_icon data:image/png;base64,

#define firefox_icon data:image/png;base64,

#define bugzilla_icon data:image/x-icon;base64,

#define mdn_icon data:image/png;base64,

#define addon_icon data:image/vndmicrosofticon;base64,
