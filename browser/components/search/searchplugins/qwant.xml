<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/">
<ShortName>Qwant</ShortName>
<Description>Search Qwant</Description>
<InputEncoding>UTF-8</InputEncoding>
<Image height="16" width="16">data:image/x-icon;base64,</Image>
<Url type="application/x-suggestions+json" method="GET" template="https://api.qwant.com/api/suggest/">
  <Param name="client" value="opensearch"/>
  <Param name="q" value="{searchTerms}"/>
</Url>
<Url type="text/html" method="GET" template="https://www.qwant.com/" rel="searchform">
  <Param name="client" value="brz-moz"/>
  <Param name="q" value="{searchTerms}"/>
</Url>
</SearchPlugin>
