{
  "name": "DuckDuckGo",
  "description": "Search DuckDuckGo",
  "manifest_version": 2,
  "version": "1.0",
  "applications": {
    "gecko": {
      "id": "ddg@search.mozilla.org"
    }
  },
  "hidden": true,
  "icons": {
    "16": "favicon.ico"
  },
  "web_accessible_resources": [
    "favicon.ico"
  ],
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "DuckDuckGo",
      "search_url": "https://duckduckgo.com/",
      "search_form": "https://duckduckgo.com/?q={searchTerms}",
      "search_url_get_params": "q={searchTerms}",
      "params": [
        {
          "name": "t",
          "condition": "purpose",
          "purpose": "contextmenu",
          "value": "ffcm"
        },
        {
          "name": "t",
          "condition": "purpose",
          "purpose": "keyword",
          "value": "ffab"
        },
        {
          "name": "t",
          "condition": "purpose",
          "purpose": "searchbar",
          "value": "ffsb"
        },
        {
          "name": "t",
          "condition": "purpose",
          "purpose": "homepage",
          "value": "ffhp"
        },
        {
          "name": "t",
          "condition": "purpose",
          "purpose": "newtab",
          "value": "ffnt"
        }
      ],
      "suggest_url": "https://ac.duckduckgo.com/ac/",
      "suggest_url_get_params": "q={searchTerms}&type=list"
    }
  }
}