{

    "manifest_version": 2,
    "name": "閉じたタブを開きなおす",
    "version": "1.0",
    "permissions": [
      "tabs",
      "sessions",
      "storage",
      "search",
      "activeTab"
    ],

      "browser_action": {
          "default_icon":  "i.svg",
          "default_title": "__MSG_action__",
          "browser_style":  true
      },
    "background": {
      "scripts": ["background.js"]
    },
  
    "applications": {
      "gecko": {
        "id": "floorp-undo-closed-tab@floorp.ablaze.one"
      }
    }
  }