# static win runtime linking
USE_STATIC_LIBS = 1

# don't use moz glue libs
MOZ_GLUE_LDFLAGS =
MOZ_GLUE_PROGRAM_LDFLAGS =

NO_PROFILE_GUIDED_OPTIMIZE = 1

include $(topsrcdir)/config/config.mk

OS_LIBS = \
	kernel32.lib \
	user32.lib \
	ole32.lib \
	shlwapi.lib \
	propsys.lib \
	advapi32.lib \
	$(NULL)

DEFINES += -DUNICODE -D_UNICODE
