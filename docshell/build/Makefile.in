DEPTH		= ../..
topsrcdir	= @top_srcdir@
srcdir		= @srcdir@
VPATH		= @srcdir@

MODULE		= docshell
IS_COMPONENT = 1

include $(DEPTH)/config/autoconf.mk

LIBRARY_NAME	= docshell
EXTRA_DSO_LIBS	= jsdom
REQUIRES	= xpcom dom shistory js necko layout uriloader timer webshell widget pref locale view intl htmlparser chardet uconv appcomps

CPPSRCS		= \
		nsDocShellModule.cpp		\
		$(NULL)

SHARED_LIBRARY_LIBS= \
		$(DIST)/lib/libbasedocshell_s.$(LIB_SUFFIX) \
		$(NULL)

EXTRA_DSO_LDOPTS= \
		-L$(DIST)/bin \
		$(EXTRA_DSO_LIBS) \
		$(MOZ_JS_LIBS) \
		$(MOZ_COMPONENT_LIBS) \
		$(NULL)

include $(topsrcdir)/config/rules.mk

INCLUDES	+= \
		-I$(srcdir)			\
		-I$(srcdir)/../base		\
		$(NULL)

