<msup>
  <mrow>
    <mo>(</mo>
    <mfrac>
      <mn>1</mn>
      <mn>2</mn>
    </mfrac>
    <mo>)</mo>
  </mrow>
  <msup>
    <mi>y</mi>
    <msub>
      <mi>a</mi>
      <mi>x</mi>
    </msub>
  </msup>
</msup>


<munder>
