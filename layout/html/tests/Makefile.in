topsrcdir	= @top_srcdir@
srcdir		= @srcdir@
VPATH		= @srcdir@

include $(DEPTH)/config/autoconf.mk


CPPSRCS = \
	TestCSSParser.cpp	\
	TestCSSPropertyLookup.cpp	\
