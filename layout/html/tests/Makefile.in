	-lraptorhtml \
	-limg \
	$(PNG_LIBS) \
	$(JPEG_LIBS) \
	-l$(MOZ_LIB_UTIL_PREFIX)util \
	-lnetlib \
	-lfileurl \
	-lftpurl \
	-lgophurl \
	-lremoturl \
	-lnetcnvts \
	-lraptorbase \
	-labouturl \
	-lhttpurl \
	-lmimetype \
	-lpwcac \
	-lnetcache \
	-lsockstuburl \
	-lnetwork \
	-lnetutil \
	-lgmbase$(MOZ_TOOLKIT) \
	$(TK_LIBS) \
	-lpref \
	-lsecfree \
	-l$(MOZ_LIB_JS_PREFIX)js \
	-ljsdom \
	-lraptorgfx \
	-lxpcom \
	-lreg \
	-lxp \
	$(ZLIB_LIBS) \
	$(NSPR_LIBS) \
	$(NULL)

