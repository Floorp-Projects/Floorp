{
    "featureFilters": {
        "brkitr_dictionaries": {
            "filterType": "exclude"
        },
        "brkitr_rules": {
            "filterType": "exclude"
        },
        "brkitr_tree": {
            "filterType": "exclude"
        },
        "confusables": {
            "filterType": "exclude"
        },
        "conversion_mappings": {
            "whitelist": [
                "ibm-37_P100-1995",
                "ibm-1047_P100-1995"
            ]
        },
        "lang_tree": {
            "filterType": "exclude"
        },
        "misc": {
            "blacklist": [
                "currencyNumericCodes",
                "genderList"
            ]
        },
        "rbnf_tree": {
            "filterType": "locale",
            "whitelist": [ "ja" ]
        },
        "region_tree": {
            "filterType": "exclude"
        },
        "stringprep": {
            "filterType": "exclude"
        },
        "translit": {
            "filterType": "exclude"
        },
        "unames": {
            "filterType": "exclude"
        }
    },
    "resourceFilters": [
        {
            "categories": [
                "zone_tree"
            ],
            "rules": [
                "-/zoneStrings/*/ec"
            ]
        },
        {
            "categories": [
                "unit_tree"
            ],
            "rules": [
                "-/*",

                "+/*/area/acre",
                "+/*/digital/bit",
                "+/*/digital/byte",
                "+/*/temperature/celsius",
                "+/*/length/centimeter",
                "+/*/duration/day",
                "+/*/angle/degree",
                "+/*/temperature/fahrenheit",
                "+/*/volume/fluid-ounce",
                "+/*/length/foot",
                "+/*/volume/gallon",
                "+/*/digital/gigabit",
                "+/*/digital/gigabyte",
                "+/*/mass/gram",
                "+/*/area/hectare",
                "+/*/duration/hour",
                "+/*/length/inch",
                "+/*/digital/kilobit",
                "+/*/digital/kilobyte",
                "+/*/mass/kilogram",
                "+/*/length/kilometer",
                "+/*/volume/liter",
                "+/*/digital/megabit",
                "+/*/digital/megabyte",
                "+/*/length/meter",
                "+/*/length/mile",
                "+/*/length/mile-scandinavian",
                "+/*/volume/milliliter",
                "+/*/length/millimeter",
                "+/*/duration/millisecond",
                "+/*/duration/minute",
                "+/*/duration/month",
                "+/*/mass/ounce",
                "+/*/concentr/percent",
                "+/*/digital/petabyte",
                "+/*/mass/pound",
                "+/*/duration/second",
                "+/*/mass/stone",
                "+/*/digital/terabit",
                "+/*/digital/terabyte",
                "+/*/duration/week",
                "+/*/length/yard",
                "+/*/duration/year",

                "+/*/compound",
                "+/*/speed/kilometer-per-hour",
                "+/*/consumption/liter-per-kilometer",
                "+/*/speed/meter-per-second",
                "+/*/consumption/mile-per-gallon",
                "+/*/speed/mile-per-hour",

                "-/*/dnam"
            ]
        },
        {
            "categories": [
                "coll_tree",
                "curr_tree",
                "locales_tree",
                "rbnf_tree",
                "unit_tree",
                "zone_tree"
            ],
            "rules": [
                "-/Version"
            ]
        },
        {
            "categories": [
                "coll_tree"
            ],
            "rules": [
                "-/collations/*/Version"
            ]
        },
        {
            "categories": [
                "curr_tree"
            ],
            "rules": [
                "-/Currencies%formal",
                "-/Currencies%variant"
            ]
        },
        {
            "categories": [
                "coll_tree"
            ],
            "rules": [
                "-/UCARules",
                "-/collations/*/Sequence"
            ]
        },
        {
            "categories": [
                "locales_tree"
            ],
            "rules": [
                "-/AuxExemplarCharacters",
                "-/Ellipsis",
                "-/ExemplarCharacters",
                "-/ExemplarCharactersIndex",
                "-/ExemplarCharactersNumbers",
                "-/ExemplarCharactersPunctuation",
                "-/MoreInformation",
                "-/NumberElements/minimalPairs",
                "-/characterLabel",
                "-/delimiters",
                "-/fields/mon",
                "-/fields/mon-narrow",
                "-/fields/mon-short",
                "-/fields/tue",
                "-/fields/tue-narrow",
                "-/fields/tue-short",
                "-/fields/wed",
                "-/fields/wed-narrow",
                "-/fields/wed-short",
                "-/fields/thu",
                "-/fields/thu-narrow",
                "-/fields/thu-short",
                "-/fields/fri",
                "-/fields/fri-narrow",
                "-/fields/fri-short",
                "-/fields/sat",
                "-/fields/sat-narrow",
                "-/fields/sat-short",
                "-/fields/sun",
                "-/fields/sun-narrow",
                "-/fields/sun-short",
                "-/measurementSystemNames"
            ]
        }
    ]
}
