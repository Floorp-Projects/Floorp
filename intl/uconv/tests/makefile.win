DEPTH=..\..\..
IGNORE_MANIFEST=1

MAKE_OBJ_TYPE   = EXE
PROGRAM = .\$(OBJDIR)\TestUConv.exe

!ifdef MOZ_DEBUG
RCFLAGS=/d_DEBUG
!endif

MISCDEP=			\
 $(DIST)\lib\xpcom32.lib	\
 $(DIST)\lib\raptorbase.lib	\
 $(DIST)\lib\uconv.lib		\
 $(LIBNSPR)			\


OBJS =					\
	.\$(OBJDIR)\nsTestUConv.obj	\
	$(NULL)

LINCS=  \
  -I$(PUBLIC)\xpcom	\
  -I$(PUBLIC)\raptor	\
  -I..\public	\
  -I..\ucvja	\
  -I..\ucvlatin	\
  $(NULL)

LLIBS=				\
 $(DIST)\lib\xpcom32.lib	\
 $(DIST)\lib\raptorbase.lib	\
 $(DIST)\lib\uconv.lib		\
 -SUBSYSTEM:CONSOLE

include <$(DEPTH)\config\rules.mak>

!ifdef MOZ_NO_DEBUG_RTL
OS_CFLAGS = $(OS_CFLAGS) -DMOZ_NO_DEBUG_RTL
!endif

install:: $(PROGRAM)
	$(MAKE_INSTALL) $(PROGRAM) $(DIST)\bin

clobber::
	rm -f $(DIST)\bin\TestUConv.exe
