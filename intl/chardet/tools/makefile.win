DEPTH = ..\..\..




MAKE_OBJ_TYPE = EXE
PROG1 = .\$(OBJDIR)\GenCyrillicClass.exe
PROGRAMS = $(PROG1)

LCFLAGS=-DUSE_NSREG
REQUIRES=\
	xpcom \
	string \
	unicharutil \
	uconv

LLIBS= \
 $(DIST)\lib\xpcom.lib \
 $(LIBNSPR)
 
include <$(DEPTH)\config\rules.mak>

install:: $(PROGRAMS)
  -for %p in ($(PROGRAMS)) do $(MAKE_INSTALL) %p $(DIST)\bin

clobber::
  -for %p in ($(PROGRAMS)) do $(RM) %p $(DIST)\bin\%p

$(PROG1): $(OBJDIR) GenCyrillicClass.cpp
