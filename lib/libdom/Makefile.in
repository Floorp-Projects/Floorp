DEPTH		= ../..
topsrcdir	= @top_srcdir@
srcdir		= @srcdir@
VPATH		= @srcdir@

include $(DEPTH)/config/autoconf.mk

MODULE		= dom
LIBRARY_NAME	= dom

REQUIRES	= js dom

EXPORTS         = dom.h domstyle.h

EXPORTS 	:= $(addprefix $(srcdir)/, $(EXPORTS))

CSRCS =	  	  domattr.c \
		  domcore.c \
		  domdoc.c \
		  domelement.c \
		  domnode.c \
		  domstyle.c \
		  domtext.c \
		  $(NULL)

include $(topsrcdir)/config/rules.mk

DEFINES += -DDOM
