@template
def Library(name):
    '''Template for libraries.'''
    LIBRARY_NAME = name


@template
def RustLibrary(name):
    '''Template for Rust libraries.'''
    Library(name)

    IS_RUST_LIBRARY = True


RustLibrary('gkrust')
