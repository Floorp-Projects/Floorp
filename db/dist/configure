  echo $ac_n "(cached) $ac_c" 1>&6
else
  case "$db_cv_path_tr" in
  /*)
  ac_cv_path_db_cv_path_tr="$db_cv_path_tr" # Let the user override the test with a path.
  ;;
  *)
  IFS="${IFS= 	}"; ac_save_ifs="$IFS"; IFS="${IFS}:"
  for ac_dir in $PATH; do
    test -z "$ac_dir" && ac_dir=.
    if test -f $ac_dir/$ac_word; then
      ac_cv_path_db_cv_path_tr="$ac_dir/$ac_word"
      break
    fi
  done
  IFS="$ac_save_ifs"
  test -z "$ac_cv_path_db_cv_path_tr" && ac_cv_path_db_cv_path_tr="missing_tr"
  ;;
esac
fi
db_cv_path_tr="$ac_cv_path_db_cv_path_tr"
if test -n "$db_cv_path_tr"; then
  echo "$ac_t""$db_cv_path_tr" 1>&6
else
  echo "$ac_t""no" 1>&6
fi

	if test "$db_cv_path_tr" = missing_tr; then
		{ echo "configure: error: No tr utility found." 1>&2; exit 1; }
	fi
fi

