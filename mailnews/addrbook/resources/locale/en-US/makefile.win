DEPTH=..\..\..\..\..

include <$(DEPTH)\config\rules.mak>

DISTBROWSER=$(DIST)\bin\chrome\messenger\locale\addressbook


install::
	$(MAKE_INSTALL) abAddressBookNameDialog.dtd $(DISTBROWSER)
	$(MAKE_INSTALL) abCardOverlay.dtd $(DISTBROWSER)
	$(MAKE_INSTALL) abCardViewOverlay.dtd $(DISTBROWSER)
	$(MAKE_INSTALL) abDirTreeOverlay.dtd $(DISTBROWSER)
	$(MAKE_INSTALL) abMainWindow.dtd $(DISTBROWSER)
	$(MAKE_INSTALL) abNewCardDialog.dtd $(DISTBROWSER)
	$(MAKE_INSTALL) abResultsTreeOverlay.dtd $(DISTBROWSER)
	$(MAKE_INSTALL) abSelectAddressesDialog.dtd $(DISTBROWSER)

clobber::
  rm -f $(DIST)\bin\chrome\messenger\locale\addressbook\abAddressBookNameDialog.dtd
  rm -f $(DIST)\bin\chrome\messenger\locale\addressbook\abCardOverlay.dtd
  rm -f $(DIST)\bin\chrome\messenger\locale\addressbook\abCardViewOverlay.dtd
  rm -f $(DIST)\bin\chrome\messenger\locale\addressbook\abDirTreeOverlay.dtd
  rm -f $(DIST)\bin\chrome\messenger\locale\addressbook\abMainWindow.dtd
  rm -f $(DIST)\bin\chrome\messenger\locale\addressbook\abNewCardDialog.dtd
  rm -f $(DIST)\bin\chrome\messenger\locale\addressbook\abResultsTreeOverlay.dtd
  rm -f $(DIST)\bin\chrome\messenger\locale\addressbook\abSelectAddressesDialog.dtd
