	rm -f $(DIST)\bin\res\samples\import-test.html
	rm -f $(DIST)\bin\chrome\messenger\content\importDialog.js
	rm -f $(DIST)\bin\chrome\messenger\content\importDialog.xul
	rm -f $(DIST)\bin\chrome\messenger\content\importProgress.js
	rm -f $(DIST)\bin\chrome\messenger\content\importProgress.xul
	rm -f $(DIST)\bin\chrome\messenger\content\fieldMapExport.js
	rm -f $(DIST)\bin\chrome\messenger\content\fieldMapExport.xul
	rm -f $(DIST)\bin\chrome\messenger\content\fieldMapImport.js
	rm -f $(DIST)\bin\chrome\messenger\content\fieldMapImport.xul



