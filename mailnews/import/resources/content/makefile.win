DEPTH=..\..\..\..

include <$(DEPTH)\config\rules.mak>

install:: 
	$(MAKE_INSTALL) import-test.html                $(DIST)\bin\res\samples
	$(MAKE_INSTALL) importDialog.js                 $(DIST)\bin\chrome\messenger\content
	$(MAKE_INSTALL) importDialog.xul                $(DIST)\bin\chrome\messenger\content
	$(MAKE_INSTALL) importProgress.xul              $(DIST)\bin\chrome\messenger\content
	$(MAKE_INSTALL) importProgress.js               $(DIST)\bin\chrome\messenger\content
	$(MAKE_INSTALL) fieldMapExport.xul              $(DIST)\bin\chrome\messenger\content
	$(MAKE_INSTALL) fieldMapExport.js               $(DIST)\bin\chrome\messenger\content
	$(MAKE_INSTALL) fieldMapImport.xul              $(DIST)\bin\chrome\messenger\content
	$(MAKE_INSTALL) fieldMapImport.js               $(DIST)\bin\chrome\messenger\content

