DEPTH		= ../../..
topsrcdir	= @top_srcdir@
srcdir		= @srcdir@
VPATH		= @srcdir@

include $(DEPTH)/config/autoconf.mk

MODULE		= browser
LIBRARY_NAME	= mozbrwsr
EXPORT_LIBRARY = 1
IS_COMPONENT	= 1
MODULE_NAME	= nsBrowserModule
REQUIRES	= xpcom \
		  string \
		  webshell \
		  necko \
		  shistory \
		  widget \
		  gfx \
		  layout \
		  content \
		  docshell \
		  appshell \
		  dom \
		  timer \
		  uriloader \
		  pref \
		  editor \
		  appcomps \
		  webbrwsr \
		  uconv \
		  chardet \
		  windowwatcher \
		  $(NULL)

CPPSRCS		= nsBrowserInstance.cpp

EXTRA_DSO_LDOPTS = \
		-L$(DIST)/bin \
		-L$(DIST)/lib \
		$(MOZ_COMPONENT_LIBS) \
		$(MOZ_JS_LIBS) \
		$(NULL)

include $(topsrcdir)/config/rules.mk

