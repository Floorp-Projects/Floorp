	$(srcdir)/NetSupportConfirmCheckYN.xul \
	$(srcdir)/NetSupportPassword.xul \
	$(srcdir)/NetSupportPrompt.xul \
	$(srcdir)/NetSupportUserPassword.xul \
	$(NULL)

install:: 
	$(INSTALL) $(EXPORT_RESOURCE_CONTENT) $(DIST)/bin/chrome/navigator/content/default

