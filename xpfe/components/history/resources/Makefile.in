DEPTH		= ../../../..
topsrcdir	= @top_srcdir@
srcdir		= @srcdir@
VPATH		= @srcdir@

include $(DEPTH)/config/autoconf.mk

CHROME_DIR	= communicator
CHROME_CONTENT_DIR = content/history
CHROME_L10N_DIR = locale/history
CHROME_SKIN_DIR = skin/history

CHROME_CONTENT	= \
		history.xul \
		history.js \
		history-test.xul \
		history-test.js \
		$(NULL)

EXPORT_RESOURCE_LOCALE	= locale/en-US/history.dtd

include $(topsrcdir)/config/rules.mk

install::
	$(INSTALL) $(EXPORT_RESOURCE_LOCALE) $(DIST)/bin/chrome/communicator/locale/history

