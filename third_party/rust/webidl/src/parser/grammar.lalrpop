CallbackRest: ast::Callback = {
    <name: "Identifier"> "=" <return_type: ReturnType> "(" <arguments: ArgumentList> ")" ";" => {
        ast::Callback {
            arguments,
            extended_attributes: vec![],
            name,
            return_type
        }
