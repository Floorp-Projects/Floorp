    <read_only: ReadOnly> <attribute: AttributeRest> => {
        ast::MixinMember::Attribute(ast::Attribute::Regular(ast::RegularAttribute {
            extended_attributes: vec![],
            inherits: false,
            name: attribute.1,
            read_only: read_only,
            type_: attribute.0
        }))
    }
};

