[package]
name = "dwrote"
description = "Lightweight binding to DirectWrite."
repository = "https://github.com/servo/dwrote-rs"
license = "MPL-2.0"
version = "0.4.0"
authors = ["Vladimir Vukicevic <vladimir@pobox.com>"]

[lib]
name = "dwrote"

[dependencies]
libc = "0.2"
lazy_static = "0.2"
winapi = "0.2"
kernel32-sys = "0.2"
gdi32-sys = "0.2"
serde = "1.0"
serde_derive = "1.0"
