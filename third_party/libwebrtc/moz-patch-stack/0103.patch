+
+'''Tool to determine inputs and outputs of a grit file.
+'''
+
+from __future__ import print_function
+
+import optparse
+import os
+import posixpath
+import sys
+
+from grit import grd_reader
+from grit import util
+
+class WrongNumberOfArguments(Exception):
+  pass
