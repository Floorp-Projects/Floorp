DEPTH		= ../..
topsrcdir	= @top_srcdir@
srcdir		= @srcdir@
VPATH		= @srcdir@

include $(DEPTH)/config/autoconf.mk

MODULE		= xpcom
LIBRARY_NAME	= xpcom

CSRCS		= plvector.c \
		  $(NULL)

CPPSRCS		= nsDebug.cpp \
		  nsHashtable.cpp \
		  nsID.cpp \
		  nsServiceManager.cpp \
		  nsSupportsArray.cpp \
		  nsRepository.cpp \
		  nsTraceRefcnt.cpp \
		  nsProxyEvent.cpp \
		  nsXPComFactory.cpp \
		  nsEventQueueService.cpp \
		  xcDll.cpp \
		  xcDllStore.cpp \
		  $(NULL)

EXPORTS		:= $(addprefix $(srcdir)/, $(EXPORTS))

CFLAGS		+= -D_IMPL_NS_COM -DUSE_NSREG

REQUIRES	= $(MODULE) libreg

include $(topsrcdir)/config/rules.mk
