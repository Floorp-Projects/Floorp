        is(win.foo, o, "should have === identity through a CrossOriginWrapper");
        SimpleTest.finish();
      }

  ]]></script>

  <iframe
      src="http://example.org/tests/js/xpconnect/tests/mochitest/file_bug658560.html"
          id="ifr"
          type="content"
          onload="go()" />
</window>
